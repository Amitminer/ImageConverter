{
    "name": "amitxd/imageconverter",
    "description": "Convert images to binary and vice versa.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "^1.10.29",
        "phpstan/phpstan-strict-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AmitxD\\ImageConverter\\": "src/"
        }
    },
    "scripts": {
        "analyze": "vendor/bin/phpstan analyze src"
    },
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "authors": [
        {
            "name": "AmitxD"
        }
    ]
}